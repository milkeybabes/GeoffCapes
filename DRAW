JUMPTAB:	DW DTRAIN	DW DBARR1	DW DBARR2	DW DTUG	DW DWOOD	DW DBELL	DW DWREST	DW DROLLDRAWSCR:	PHA	JSR TSCREEN	JSR DCLOUDS	PLA		AND #7	ASL A	TAY	LDA JUMPTAB,Y	STA DJUMP	LDA JUMPTAB+1,Y	STA DJUMP +1	JMP (DJUMP)LOADS:		; DRAW SPRITES X=X+SPXSIZE		; UNTIL X>XMAX	JSR IMEOR	PHA	TXA	CLC	ADC SPXSIZE	TAX	PLA	CPX SPXMAX	BCC LOADS	RTSDBARR1:	; SCREEN 1	JSR TREES	LDA #6	; BRICKS	STA BLSPRITE	LDX #12	LDY #16	LDA #17	JSR ALONG	LDX #45	LDA #17	LDY #128	JSR IMAGE		LDA #19	; BOX ????	STA BLSPRITE	LDA #1	LDX #28	LDY #15	JSR ALONG	LDX #34	JSR ALONG	LDX #40	JSR ALONG	LDA #1		; BARMEN	JSR PARAM	LDA #6	LDX #23	LDY #86	JMP IMAGEDBARR2:	LDA #6	; SCREEN 2	STA BLSPRITE	LDX #12	LDY #16	LDA #17	JSR ALONG	LDY #128-(8*6)END_PIECE:	LDX #45	LDA #17	JSR IMAGE	TYA	CLC	ADC #8	TAY	CMP #128+8	BNE END_PIECE	LDA #15	LDY #16	LDX #17DBL1:	DEY	JSR ALONG		SEC	SBC #1	INX	INX	CPY #10	BNE DBL1	LDA #15		; LORRY	JSR PARAM	LDA #0	LDX #41	LDY #60	JSR IMAGE	LDA #1		; BARMEN	JSR PARAM	LDA #1	LDX #11	LDY #86	JSR IMAGE 	RTSDTUG:	JSR TREES	LDA #10	STA BLSPRITE	LDA #18	LDY #13	LDX #12TUG_M:	JSR ALONG	INY	CPY #17	BNE TUG_M	LDX #45-10	LDY #96+8	LDA #9	JSR IMAGE	LDA #1	STA FLOP	LDA #9	LDX #11+10	JSR IMAGE	LDX #45-12	LDA #0	STA FLOP	LDA #9	LDY #96+16	JSR IMAGE	LDA #1	STA FLOP	LDA #9	LDX #11+12	JSR IMAGE	LDA #0	STA FLOP	LDA #11	;PIECE OF WATER EDGE	LDX #45-14	LDY #96+16	JSR IMAGE	LDA #1	STA FLOP	LDA #11	LDX #11+14	JSR IMAGE	LDA #12	;WATER IT SELF	LDX #11+16	JSR IMAGE	LDX #11+18	JSR IMAGE	LDA #18	;WIPE OUT TOP PIECE	STA BLSPRITE	LDX #24	LDY #13	LDA #6	JSR ALONG	LDY #14	;WIPE OUT MIDDLE PIECE	LDX #26	LDA #4	JSR ALONG	LDA #0	STA FRAME	LDA #4	STA FRAME1	LDX #16TUGIES:	STX &3E		LDA #2		; TUGMEN	JSR PARAM	LDX &3E	; GEOFF	LDY #69	LDA FRAME	JSR IMEOR	LDA &3E	CLC	ADC #38-14	TAX	PHA	LDA FRAME1	JSR IMEOR	; OTHER MAN		LDA #10	; SOME ROPE		JSR PARAM	LDA #2	STA SPXSIZE	PLA	STA SPXMAX	LDA #5	; ROPE IN CHOPBIT	LDY #82	LDX &3E	INX	INX	INX	INX	JMP LOADSROPE_DOWN:	LDX #11	LDA #11+10	STA SPXMAX	JMP ROPE_ME_UPROPE2_DOWN:	LDX #37	LDA #37+10	STA SPXMAXROPE_ME_UP:	LDA #10	JSR PARAM	LDA #5	; ROPE IN CHOPBIT	LDY #100	JMP LOADSDCROWDS:	LDA #8	JSR PARAM	LDA #6	STA SPXSIZE	LDA #45	STA SPXMAX	LDX #11	LDY #20	LDA #2	JMP LOADS	TREES:	LDA #8	JSR PARAM	LDA #6	STA SPXSIZE	LDA #45	STA SPXMAX	LDX #11	LDY #20	LDA #1	JMP LOADSDWOOD:	JSR TREES	LDA #10		; CHOPBIT	JSR PARAM	LDY #88	STY TEMPY1	LDX #12METAL:	LDA #4	JSR IMAGE	INY	INY	INY	INY	CPY #88+(10*4)	BNE METAL		LDY #0		STY TEMPY2	LOGS:	LDX #14STRIP:	LDY TEMPY2	LDA LOG_DATA,Y	LDY TEMPY1	JSR IMAGE	INC TEMPY2	INX	INX	CPX #14+(2*16)	BNE STRIP	LDA TEMPY1	CLC	ADC #4	STA TEMPY1	CMP #88+(10*4)	BNE LOGS	LDX #44	LDY #88METAL2:	LDA #4	JSR IMAGE	INY	INY	INY	INY	CPY #88+(10*4)	BNE METAL2	RTSDBELL:	JSR DCROWDS	LDA #8	JSR PARAM	LDX #11	LDY #20	LDA #2	JSR IMEOR	LDA #0	JSR PARAM	LDX #12	LDY #126	LDA #14	JSR IMAGE	LDA #13	LDX #14	JSR IMAGE	LDA #15	STA BLSPRITE	LDA #12	LDX #13	LDY #4	JSR DOWN	LDY #50	LDX #13	LDA #16	JSR IMAGE	LDA #1	STA UPSIDE	LDA #16	LDY #50-8	JMP IMAGEDWREST:	JSR PLATFORM		JMP DCROWDSDROLL:	JSR PLATFORM	JSR DCROWDS	LDA #6	JSR PARAM	; CAR FACE ON	LDA #0	LDX #20	LDY #90	JSR IMAGE	LDA #5	JSR PARAM	LDA #0	LDX #25	LDY #90	JMP IMAGE	; MAN POS. 1DTRAIN:	; SCREEN 0	JSR PLATFORM		LDA #13	; BELL BAR *_______*	JSR PARAM	LDX #30	LDY #80	LDA #0	JSR IMEOR	LDA #14	; 56KG WEIGHT	JSR PARAM	LDA #0	LDX #41	LDY #103	JSR SBACK	LDA #12	; BOX HORSE	JSR PARAM	LDA #0	LDX #21	LDY #65	JSR SBACK	LDA #1	STA FLOP	LDA #0	LDX #24	JSR SBACK		JMP DCROWDS	; CROWDSDCLOUDS:	LDA #19		; CLOUDS	JSR PARAM	LDX #12	LDA #0	LDY #9	JSR IMAGE	LDA #1	LDX #25	INY	JSR IMAGE	LDX #41	LDA #2	INY	JMP IMAGEPLATFORM:	JSR vdu	DB 25,4	DW 150,550	DB 25,5	DW 390,730	DB 25,5	DW 390+500,730	DB 25,5	DW 1280-150,550	DB 25,5	DW 150,550	DB 25,85	DW 1280-150,530	DB 25,85	DW 150,530	NOP	RTS		EXTEND ':2.R.EVENTS'